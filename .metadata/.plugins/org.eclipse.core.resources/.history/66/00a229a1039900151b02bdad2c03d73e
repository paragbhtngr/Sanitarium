package state;

import java.awt.Graphics;

import entities.creatures.Player;
import game.Game;

public class GameState extends State{
	
	private Player player;
	
	public GameState(Game game){
		super(game);
		player = new Player(0,0);
	}
		
	@Override
	public void tick() {		
	}

	@Override
	public void render(Graphics g) {
		player.render(g);
	}
	
}
