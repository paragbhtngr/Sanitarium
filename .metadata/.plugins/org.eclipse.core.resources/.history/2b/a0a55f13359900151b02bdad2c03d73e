package entities;

import java.awt.Graphics;

public abstract class Entity {
	
	protected float x, y;
	
	public Entity(double x, double y){
		this.x = x;
		this.y = y;
	}
	
	public abstract void tick();
	
	public abstract void render(Graphics g);
	
	public double getX(){
		return this.x;
	}
	
	public double getY(){
		return this.y;
	}
}
